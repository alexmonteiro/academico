<h1>Listing issuing_institutions</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Acronym</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @issuing_institutions.each do |issuing_institution| %>
  <tr>
    <td><%= issuing_institution.description %></td>
    <td><%= issuing_institution.acronym %></td>
    <td><%= link_to 'Show', issuing_institution %></td>
    <td><%= link_to 'Edit', edit_issuing_institution_path(issuing_institution) %></td>
    <td><%= link_to 'Destroy', issuing_institution, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

