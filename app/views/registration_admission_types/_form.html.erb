<%= form_for @registration_admission_type, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :registration_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :registration_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :admission_type_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :admission_type_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                registration_admission_types_path, :class => 'btn' %>
  </div>
<% end %>
