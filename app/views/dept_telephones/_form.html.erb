<%= form_for @dept_telephone, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :telephone_type_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :telephone_type_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :dept_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :dept_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :area_code, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :area_code, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :number, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :number, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :branch, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :branch, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                dept_telephones_path, :class => 'btn' %>
  </div>
<% end %>
