<%- model_class = @teaching_equipament.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @teaching_equipament.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:arrival_date) %>:</strong></dt>
  <dd><%= @teaching_equipament.arrival_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:equipament_type_id) %>:</strong></dt>
  <dd><%= @teaching_equipament.equipament_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:situation_teaching_equipament_id) %>:</strong></dt>
  <dd><%= @teaching_equipament.situation_teaching_equipament_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dept_id) %>:</strong></dt>
  <dd><%= @teaching_equipament.dept_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              dept_teaching_equipaments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_dept_teaching_equipament_path, :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              dept_teaching_equipament_path,
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
