<%= form_for @class_time, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :started_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :started_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ended_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ended_at, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                class_times_path, :class => 'btn' %>
  </div>
<% end %>
