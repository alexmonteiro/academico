<%= form_for ([@techaxis_type, @techaxis], :html => { :class => 'form-horizontal' }) do |f| %>
  <div class="control-group">
    <%= f.label :techaxis, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :techaxis, :class => 'text_field' %>
    </div>
  </div>
  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                techaxis_type_techaxes_path(), :class => 'btn' %>
  </div>
<% end %>
