<h1>Listing techaxes</h1>

<table>
  <tr>
    <th>Techaxis</th>
    <th>Techaxis type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @techaxes.each do |techaxis| %>
  <tr>
    <td><%= techaxis.techaxis %></td>
    <td><%= techaxis.techaxis_type %></td>
    <td><%= link_to 'Show', techaxis %></td>
    <td><%= link_to 'Edit', edit_techaxis_path(techaxis) %></td>
    <td><%= link_to 'Destroy', techaxis, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

