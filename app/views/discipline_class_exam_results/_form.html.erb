<%= form_for @discipline_class_exam_result, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :registration_class_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :registration_class_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :discipline_class_exam_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :discipline_class_exam_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :concept, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :concept, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :result, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :result, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                discipline_class_exam_results_path, :class => 'btn' %>
  </div>
<% end %>
